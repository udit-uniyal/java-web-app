pipeline {
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Build') {
      agent any
      steps {
        sh 'docker build -t darinpope/java-web-app:latest .'
      }
    }
    stage('Scan') {
      agent any
      steps {
        sh 'trivy darinpope/java-web-app:latest'
      }
    }
  }
}
